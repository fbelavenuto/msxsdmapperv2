set -tmpdir "/tmp"
set -xsthdpdir "xst"
run
-ifn sdmapper.prj
-ifmt mixed
-ofn sdmapper
-ofmt NGC
-p xc9500xl
-top sdmapper
-opt_mode Speed
-opt_level 2
-iuc NO
-keep_hierarchy Yes
-netlist_hierarchy As_Optimized
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case Maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-mux_extract Yes
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-pld_ce YES
-wysiwyg NO
-equivalent_register_removal YES
